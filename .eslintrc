{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "eqeqeq": 2,
        "no-trailing-spaces": 2,
        "indent": 0,
        "no-mixed-spaces-and-tabs": 1,
        "no-var": 1
    },
    "globals": {
        "$": true
    }
}